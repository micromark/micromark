{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": ["html", "clear-text", "progress"],
  "mutate": [
    "packages/{build,micromark-util-combine-extensions,micromark-util-encode,micromark-util-html-tag-name,micromark-util-resolve-all,micromark-util-symbol}/*.js",
    "packages/*/dev/**/*.js"
  ],
  "testRunner": "command",
  "coverageAnalysis": "off",
  "commandRunner": {"command": "npm run test-api"},
  "timeoutFactor": 3
}
