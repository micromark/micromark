{
  "name": "micromark",
  "version": "0.0.1",
  "description": "markdown parser under @unifiedjs thatâ€™s different",
  "main": "built/lib/index.js",
  "types": "built/lib/index.d.ts",
  "module": "lib/index.ts",
  "scripts": {
    "build": "rimraf built && tsc",
    "watch": "tsc --watch",
    "lint": "tslint -p ./tsconfig.json",
    "test": "jest",
    "prettier": "prettier --config ./.prettierrc --write \"{test,bin,lib}/**/*.{ts,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micromark/micromark.git"
  },
  "keywords": [
    "markdown",
    "lexer",
    "lexing",
    "unifiedjs"
  ],
  "author": "",
  "contributors": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/micromark/micromark/issues"
  },
  "homepage": "https://github.com/micromark/micromark#readme",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.9",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {},
  "jest": {
    "roots": [
      "<rootDir>/lib/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/**/*.ts"
    ]
  }
