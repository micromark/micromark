    "Victor Felder <victor@draft.li> (https://draft.li)",
    "Mudit Ameta <zeusdeux@gmail.com> (https://mudit.xyz)"
  ],
  "license": "MIT",
  "bugs": "https://github.com/micromark/micromark/issues",
  "homepage": "https://unified.js.org",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.9",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {},
  "jest": {
    "roots": [
      "<rootDir>/lib/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
